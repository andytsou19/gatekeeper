{
    "name": "Gatekeeper",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "stop.html",
        "default_icon": "stop.png"
      },
    "permissions": ["storage","tabs"],
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": ["stop.js"]
        }
      ] 
  }